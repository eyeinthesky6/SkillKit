{
  "globalExclusions": {
    "patterns": [
      {
        "type": "PYTHON_NOTIMPLEMENTED_CONSTANT",
        "match": "return NotImplemented.*comparison|__eq__|__lt__|__gt__|__le__|__ge__",
        "reason": "Exclude legitimate NotImplemented usage in rich comparison methods"
      },
      {
        "type": "PYTHON_PASS_ONLY_FUNCTION",
        "match": "@abstractmethod|abc\\.abstractmethod|def __.*__|def _.*_.*__",
        "reason": "Exclude abstract methods and dunder methods that legitimately use only pass"
      },
      {
        "type": "PYTHON_PASS_ONLY_FUNCTION",
        "match": "def test_.*|def _test_.*",
        "reason": "Exclude test methods that might legitimately be empty stubs"
      },
      {
        "type": "PYTHON_DOCSTRING_TODO",
        "match": "This method|This function|This class.*TODO.*done|TODO.*already",
        "reason": "Exclude docstrings that mention TODO in descriptive context, not incomplete work"
      },
      {
        "type": "PYTHON_ELLIPSIS",
        "match": "\\.\\.\\..*type.*hint|Type\\[.\\.\\.",
        "reason": "Exclude ellipsis in type hints (legitimate Python syntax)"
      },
      {
        "type": "PYTHON_PRAGMA_NO_COVER_TODO",
        "match": "pragma.*no.*cover.*test|pragma.*no.*cover.*unreachable",
        "reason": "Exclude legitimate coverage suppressions for test/unreachable code"
      },
      {
        "type": "PYTHON_TYPE_IGNORE_TODO",
        "match": "type.*ignore.*typing|type.*ignore.*mypy",
        "reason": "Exclude legitimate type ignore comments for typing issues"
      },
      {
        "type": "PYTHON_PYLIN_DISABLE_TODO",
        "match": "pylint.*disable.*unused|pylint.*disable.*import",
        "reason": "Exclude legitimate pylint disables for common patterns"
      },
      {
        "type": "PYTHON_NOQA_TODO",
        "match": "noqa.*unused|noqa.*import|noqa.*redefined",
        "reason": "Exclude legitimate noqa comments for common linting issues"
      },
      {
        "type": "PYTHON_RETURN_NONE_ONLY",
        "match": "def __.*__|def _.*__.*|def test_.*",
        "reason": "Exclude dunder methods and test methods that legitimately return None"
      },
      {
        "type": "PYTHON_RETURN_EMPTY_LIST",
        "match": "def __.*__|def _.*__.*|def test_.*",
        "reason": "Exclude dunder methods and test methods that legitimately return empty lists"
      },
      {
        "type": "PYTHON_RETURN_EMPTY_DICT",
        "match": "def __.*__|def _.*__.*|def test_.*",
        "reason": "Exclude dunder methods and test methods that legitimately return empty dicts"
      },
      {
        "type": "PYTHON_SUPER_ONLY_METHOD",
        "match": "def __init__|def __new__|def __del__",
        "reason": "Exclude constructor/destructor methods that often just call super()"
      },
      {
        "type": "PYTHON_PASS_ONLY_CLASS",
        "match": "class.*Exception|class.*Error",
        "reason": "Exclude exception classes that legitimately just pass"
      },
      {
        "type": "PYTHON_PLACEHOLDER_COMMENT",
        "match": "placeholder.*test|test.*placeholder",
        "reason": "Exclude legitimate test placeholders"
      }
    ]
  }
}
